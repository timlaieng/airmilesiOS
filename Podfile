    pod 'Fabric'
pod 'Crashlytics'
pod 'RealmSwift'
pod 'KeychainSwift'
end

  target 'MileageCalculatorTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MileageCalculatorUITests' do
    inherit! :search_paths
    # Pods for testing
  end
